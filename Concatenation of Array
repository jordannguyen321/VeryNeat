#include <stdio.h>
#include <string.h>
#include <stdint.h>

// void getConcatenation(int* nums, int numsSize, int* ans, int ansSize) {
//     //nums array, nums size, ans array, ans size
//     //input nums array, output ans array
//     //ex: nums= 1,2,1,4, ans = 1,2,1,4,1,2,1,4
    
        
//         for(int j = 0; j <numsSize; j++){
//             ans[j] = nums[j];
//             ans[j+numsSize] = nums[j];
//         }
